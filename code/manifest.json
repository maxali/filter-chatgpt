{
  "manifest_version": 3,
  "name": "ChatGPT Chats Filter",
  "version": "1.3",
  "description": "Easily filter and manage your ChatGPT conversations with this intuitive Chrome extension.",
  "permissions": [],
  "icons": {
    "16": "icons/icon_16x16.png",
    "48": "icons/icon_48x48.png",
    "128": "icons/icon_128x128.png"
  },
  "action": {
    "default_icon": "icons/icon_128x128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"]
    }
  ]
}
